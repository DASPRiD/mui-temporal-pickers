    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.4",
    "@mui/x-date-pickers": "^8.15.0",
    "@tsconfig/node22": "^22.0.2",
    "@tsconfig/vite-react": "^7.0.1",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react-swc": "^4.2.0",
    "c8": "^10.1.3",
